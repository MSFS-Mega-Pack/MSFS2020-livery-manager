{
  "name": "fs2020-livery-manager",
  "productName": "fs2020-livery-manager",
  "version": "1.0.0",
  "description": "A simple, extensible livery manager for Microsoft Flight Simulator",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.22.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "yarn build",
    "postpackage": "electron-packager ./ --out=./release-builds",
    "format": "prettier --write \"**/*.{js,json,less,css,md}\"",
    "format:check": "prettier --loglevel debug --check \"**/*.{js,json,less,css,md}\"",
    "eslint": "npx eslint .",
    "eslint:fix": "yarn eslint --fix"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@material-ui/core": "^4.11.0",
    "electron-json-config": "^1.5.3",
    "electron-react-titlebar": "^0.8.2",
    "fontsource-ibm-plex-mono": "^3.0.5",
    "fontsource-poppins": "^3.0.3",
    "mdi-react": "^7.3.0",
    "open": "^7.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^5.2.0",
    "react-router-transition": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/eslint-parser": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "electron": "^10.0.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^15.1.0",
    "eslint": "^7.7.0",
    "eslint-plugin-jsdoc": "^30.3.2",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.10.0",
    "prettier": "^2.1.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
