{
  "name": "fs2020-livery-manager",
  "productName": "Liveries Mega Pack Manager",
  "version": "0.4.6",
  "description": "A simple, extensible livery manager for Microsoft Flight Simulator",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "David Wheatley",
    "email": "hi@davwheat.dev",
    "url": "https://github.davwheat.dev/"
  },
  "contributors": [
    {
      "name": "Jaap Rodenburg",
      "url": "https://github.com/GewoonJaap"
    },
    {
      "name": "Fraser Donaldson",
      "url": "https://github.com/NinjaLabs-Dev",
      "email": "fraser@ninjalabs.co.uk"
    }
  ],
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.22.0"
  },
  "browserslist": [
    "last 1 Electron version"
  ],
  "main": "./main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack serve --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "prebuild": "yarn clean",
    "build": "echo 'Starting build' && cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "clean": "cross-env rimraf ./dist/ ./publish/ || true",
    "prepackage": "yarn build",
    "package": "electron-packager ./ --out=./publish --asar --icon=\"./assets/icon.ico\" --protocol=megapack --protocol-name=\"Liveries Mega Pack Manager\"",
    "postpackage": "electron-installer-windows --config installer.config.json",
    "format": "prettier --write \"**/*.{js,json,less,css,md}\"",
    "format:check": "prettier --loglevel debug --check \"**/*.{js,json,less,css,md}\"",
    "eslint": "npx eslint .",
    "eslint:fix": "yarn eslint --fix"
  },
  "dependencies": {
    "@fontsource/ibm-plex-mono": "^4.2.1",
    "@fontsource/poppins": "^4.2.1",
    "@material-ui/core": "^4.11.3",
    "@sentry/electron": "^2.3.0",
    "@sentry/integrations": "^6.2.0",
    "adm-zip": "^0.5.3",
    "clsx": "^1.1.1",
    "core-js": "^3.9.1",
    "dayjs": "^1.10.4",
    "del": "^6.0.0",
    "electron-json-config": "^1.5.3",
    "electron-react-titlebar": "^0.8.2",
    "electron-squirrel-startup": "^1.0.0",
    "mdi-react": "^7.4.0",
    "node-fetch": "^2.6.1",
    "node-rsa": "^1.1.1",
    "notistack": "^1.0.5",
    "open": "^7.4.2",
    "overlayscrollbars": "^1.13.1",
    "overlayscrollbars-react": "^0.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-router-transition": "^2.1.0",
    "react-virtuoso": "^1.5.10",
    "regenerator-runtime": "^0.13.7",
    "request": "^2.88.2",
    "request-progress": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.12.13",
    "@sentry/webpack-plugin": "^1.14.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.0",
    "dotenv": "^8.2.0",
    "electron": "^11.3.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.2.0",
    "eslint": "^7.21.0",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.2.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.24.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
